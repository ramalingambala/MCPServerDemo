{
    "mcpServers": {
        "fetch": {
            "command": "python3",
            "args": [
                "-m",
                "mcp_server_fetch"
            ],
            "env": {
                "PYTHONPATH": "/opt/homebrew/opt/python@3.13/bin"
            }
        }
    },
    "markdown-tools": {
        "command": "npx",
        "args": [
            "-y",
            "@example/markdown-mcp"
        ]
    },
    "tool": "tool-name",
    "arguments": {
        "argument-name": "value"
    }
}